<h1>New User</h1>

<%= form_for :user, url: user_index_path do |f| %>
<p>
  <%= f.label :name %><br/>
  <%= f.text_field :name %>
</p>
    <p>
      <%= f.label :soname %><br/>
      <%= f.text_field :soname %>
    </p>
    <p>
      <%= f.label :email %><br/>
      <%= f.text_field :email %>
    </p>
    <p>
      <%= f.label :address %><br/>
      <%= f.text_field :address %>
    </p>
    <p>
      <%= f.submit %>
    </p>
<% end %>
