<h1>Users</h1>
<%= link_to 'Create User', new_user_path %>
<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
  </tr>

  <% @user.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', user_path(user) %></td>
      </tr>
  <% end %>
</table>
